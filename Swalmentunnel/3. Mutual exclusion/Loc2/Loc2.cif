    edge u_primairVBopen when VB1_BedieningVRI.auto and VB1_BedieningAB.auto and (not VB1_PrimairVRI.uit or not VB1_PrimairAB.open) goto primairVBopen;
    edge u_VRIrood when VB1_BedieningVRI.hand goto VRIrood;
    edge u_VRIknipper when VB1_BedieningVRI.hand goto VRIknipper;
    edge u_VRIuit when VB1_BedieningVRI.hand goto VRIuit;
    edge u_ABstop when VB1_BedieningAB.hand goto ABstop;
    edge u_ABopen when VB1_BedieningAB.hand goto ABopen;
    edge u_ABsluit when VB1_BedieningAB.hand goto ABsluit;
    edge u_ABsluitOnv when VB1_BedieningAB.hand goto ABsluitOnv;
