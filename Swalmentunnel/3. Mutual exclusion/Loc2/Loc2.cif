    edge c_primairVRIrood_klaar when VB1_PrimairVRI.rood goto los;
  location primairVRIuit:
    edge c_primairVRIuit_klaar when VB1_PrimairVRI.uit goto los;
  location primairVBdicht:
    edge c_primairVBdicht_klaar when VB1_PrimairVRI.rood and VB1_PrimairAB.dicht goto los;
  location primairVBopen:
    edge c_primairVBopen_klaar when VB1_PrimairVRI.uit and VB1_PrimairAB.open goto los;
