  uncontrollable u_auto;
  uncontrollable u_hand;
  controllable c_auto_klaar;
  controllable c_hand_klaar;
  location los:
    initial;
    marked;
    edge u_auto goto auto;
    edge u_hand goto hand;
  location auto:
    edge c_auto_klaar when VB2_BedieningVRI.auto goto los;
  location hand:
    edge c_hand_klaar when VB2_BedieningVRI.hand goto los;
end
