    edge c_VRIrood_klaar when VB1_ToegangVRI.rood goto los;
  location VRIknipper:
    edge c_VRIknipper_klaar when VB1_ToegangVRI.knipper goto los;
  location VRIuit:
    edge c_VRIuit_klaar when VB1_ToegangVRI.uit goto los;
  location ABstop:
    edge c_ABstop_klaar when VB1_ToegangAB.stop goto los;
  location ABopen:
    edge c_ABopen_klaar when VB1_ToegangAB.op goto los;
  location ABsluit:
    edge c_ABsluit_klaar when VB1_ToegangAB.neer goto los;
  location ABsluitOnv:
    edge c_ABsluitOnv_klaar when VB1_ToegangAB.neeronvoorwaardelijk goto los;
  location resetHD:
    edge c_resetHD_klaar when VB1_HoogteDetectieRood_Lampen.uit goto los;
end
